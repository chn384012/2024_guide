    "\n",
    "X_train = df_train.loc[:, col]\n",
    "y_train = df_train[['Survived']]\n",
    "X_test = df_test.loc[:, col]\n",
    "\n",
    "print(X_train.shape)\n",
    "print(y_train.shape)\n",
    "print(X_test.shape)\n",
