    "'''7'''\n",
    "# 2. 객실 등급(Pclass)에 따른 생존율\n",
    "survival_rate = df_train.groupby('Pclass')['Survived'].mean().reset_index()  \n",
    "plt.figure(figsize=(10, 6))\n",
